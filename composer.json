{
  "name": "kleegroup/franceconnect-bundle",
  "type": "symfony-bundle",
  "description": "Symfony4 OpenID security extension franceconnect",
  "keywords": ["Security", "OpenId", "Authentication"],
  "homepage": "https://github.com/KleeGroup/FranceConnect-Symfony",
  "license": "MIT",
  "authors": [
    {
      "name": "Klee Group",
      "homepage": "http://www.kleegroup.com/"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "ext-json": "*",
    "namshi/jose": "7.2.*",
    "mashape/unirest-php": "3.*",
    "symfony/config": "3.4.* || >=4.0",
    "symfony/dependency-injection": "3.4.* || >=4.0",
    "symfony/framework-bundle": "3.4.* || >=4.0",
    "symfony/http-foundation": "3.4.* || >=4.0",
    "symfony/http-kernel": "3.4.* || >=4.0",
    "symfony/routing": "3.4.* || >=4.0",
    "symfony/security-bundle": "3.4.* || >=4.0",
  },
  "require-dev": {
    "phpunit/phpunit": "~7.1"
  },
  "autoload": {
    "psr-4": { "KleeGroup\\FranceConnectBundle": "" }
  },
  "autoload-dev": {
    "psr-4": { "KleeGroup\\FranceConnectBundle\\Tests\\": "Tests/" }
  },
  "suggest": {
    "doctrine/orm": "If you want store identities to database",
    "doctrine/mongodb-odm": "If you want store identities to mongodb"
  },
  "target-dir": "KleeGroup/FranceConnectBundle",
  "config": {
    "bin-dir": "bin",
    "sort-packages": true
  }
}